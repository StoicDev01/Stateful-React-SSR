{
  "name": "web-generator",
  "version": "1.0.0",
  "type": "module",
  "module": "es2022",
  "tsup": {
    "format": "esm",
    "target": "es2022"
  },
  "scripts": {
    "dev": "tsx --watch ./src/Server.ts",
