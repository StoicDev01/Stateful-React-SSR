  "author": "izkeas <izkeasyomito@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "faunadb": "^4.7.1",
    "glob": "^8.1.0",
    "lodash-es": "^4.17.21",
    "node-schedule": "^2.1.1",
    "serve-static": "^1.15.0",
    "tsc": "^2.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/glob": "^8.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node-schedule": "^2.1.0",
    "@types/uuid": "^9.0.0",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
